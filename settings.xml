<?xml version="1.0" encoding="utf-8"?>
<settings styleversion="2.3.3">	
	<template_path>eqdkp_bluesky</template_path>
	<attendees_columns>6</attendees_columns>
	<logo_position>left</logo_position>
	<favicon_img></favicon_img>
	<banner_img>../../../../../templates/eqdkp_bluesky/images/template_background.jpg</banner_img>
	<body_background>#0e0e0e</body_background>
	<background_pos>normal</background_pos>
	<background_type>0</background_type>
	
	<body_link>#FFCF02</body_link>
	<body_link_style>none</body_link_style>
	<body_hlink>#000</body_hlink>
	<body_hlink_style>none</body_hlink_style>
	
	<header_link>#FFFFFF</header_link>
	<header_link_style>none</header_link_style>
	<header_hlink>#FFCF02</header_hlink>
	<header_hlink_style>none</header_hlink_style>
	
	<tr_color1>#7D7D7D</tr_color1>
	<tr_color2>#303030</tr_color2>
	<th_color1>#D9D7D7</th_color1>
	<fontface1>Verdana, Arial, Helvetica, sans-serif</fontface1>
	<fontface2>Verdana, Arial, Helvetica, sans-serif</fontface2>
	<fontface3>Verdana, Arial, Helvetica, sans-serif</fontface3>
	<fontsize1>10</fontsize1>
	<fontsize2>11</fontsize2>
	<fontsize3>12</fontsize3>
	<fontcolor1>#D9D7D7</fontcolor1>
	<fontcolor2>#FFFFFF</fontcolor2>
	<fontcolor3>#000000</fontcolor3>
	<fontcolor_neg>#FF0000</fontcolor_neg>
	<fontcolor_pos>#008800</fontcolor_pos>
	
	<table_border_width>1</table_border_width>
	<table_border_color>#999999</table_border_color>
	<table_border_style>solid</table_border_style>
	
	<table_th_background_color>rgba(0, 0, 0, 0.65)</table_th_background_color>
	<table_th_font_color>rgb(159, 159, 159)</table_th_font_color>
	<table_tr_font_color>rgb(159, 159, 159)</table_tr_font_color>
	<table_tr_background_color1>rgba(125, 125, 125,0)</table_tr_background_color1>
